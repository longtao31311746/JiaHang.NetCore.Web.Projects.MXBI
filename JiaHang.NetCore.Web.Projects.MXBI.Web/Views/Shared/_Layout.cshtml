<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - MXBI.Web</title>
    <link href="~/layui/css/layui.css" rel="stylesheet" />
    <link href="https://at.alicdn.com/t/font_1018241_zxgexn67aas.css" rel="stylesheet" />
 
    <link rel="stylesheet" href="~/common/element-ui/theme-chalk/element.css">
    <link rel="stylesheet" href="~/common/app.css" />

    <script src="~/common/vue.min.js"></script>
    <script src="~/common/element-ui/element.js"></script>
    <script src="~/common/axios.min.js"></script>
    <script src="~/layui/layui.js"></script>

    <link rel="stylesheet" href="https://unpkg.com/element-ui/lib/theme-chalk/index.css">
    <link href="~/layui/css/dist/formSelects-v4.css" rel="stylesheet" />
</head>
<body>

    <div class="layui-layout layui-layout-admin">

        @await Html.PartialAsync("_header")
        @await Html.PartialAsync("_side")

        <div class="layui-body" style="bottom:0px;">

            <!-- 内容主体区域 -->
            <div style="padding: 15px;background-color: #f1f1f1;">
                @RenderBody()
            </div>
        </div>

        @*<div class="layui-footer">


                © layui.com - 底部固定区域
            </div>*@

    </div>

</body>
</html>
